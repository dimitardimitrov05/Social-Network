@model List<Connectly.Models.FriendshipViewModels.FriendsViewModel>;

<div class="mt-5">
    <div class="card">
        <div class="card-body">
            <div style="margin-top: 100px">
                @foreach (var friend in Model)
                {
                    <h4>@friend.FirstName @friend.LastName</h4>
                }
            </div>
        </div>
    </div>
</div>